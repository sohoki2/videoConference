	<bean id="resEaiScheduleSchedulering"
		class="org.springframework.scheduling.quartz.MethodInvokingJobDetailFactoryBean">
		<property name="targetObject" ref="resStateCreateSchedulerService" />
		<property name="targetMethod" value="resTimeCreateStateSchede" />
		<property name="concurrent" value="false" />
	</bean> 
	
	<bean id="resEaiScheduleScheduleringTrigger"
		class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">
		<property name="jobDetail" ref="resEaiScheduleSchedulering" />
		<property name="cronExpression" value="0 0 23 * * ?" />
	</bean>
	
	<bean id="resEaiScheduleScheduler" class="org.springframework.scheduling.quartz.SchedulerFactoryBean">
		<property name="triggers">
			<list>
				<ref bean="resEaiScheduleScheduleringTrigger" />
			</list>
		</property>
	</bean> 
	
</beans>
