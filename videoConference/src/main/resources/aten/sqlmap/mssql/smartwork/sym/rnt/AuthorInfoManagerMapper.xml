<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sohoki.backoffice.sym.rnt.mapper.AuthorInfoManagerMapper">

    <select id="selectAuthorIInfoManageCombo"  resultType="AuthorInfo">
	       SELECT  AUTHOR_CODE as authorCode, AUTHOR_NM as authorNm
	       FROM LETTNAUTHORINFO       
	       ORDER BY  AUTHOR_CODE  ASC
    </select>
</mapper>